@using EntityLayer.Concrete
@model List<User>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/MovieLayout.cshtml";
}

@foreach(var item in Model)
{
    <table>
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Mail</td>
            <td>@item.Password</td>
        </tr>
    </table>
}

