@model IEnumerable<EntityLayer.Concrete.Serie>
<div class="slick-multiItemSlider">
	@foreach (var item in Model)
	{
		<div class="movie-item">
			<div class="mv-img">
				<a href="#"><img src="@item.Image" alt="" width="285" height="437"></a>
			</div>
			<div class="title-in">
				<div class="cate">
					@*<span class="@item.Category.color"><a href="#">@item.Category.Name</a></span>*@
				</div>
				<h6><a href="/Serie/SerieDetails/@item.Id">@item.Name</a></h6>
				<p><i class="ion-android-star"></i><span>@item.Star</span> /10</p>
				<h6><a href="/Serie/SerieDetails/@item.Id">@item.Season Season</a></h6>
			</div>
		</div>
	}
</div>